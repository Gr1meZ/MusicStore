@model IEnumerable<MusicStore.Data.ApplicationUser>
@inject Microsoft.AspNetCore.Mvc.Localization.IViewLocalizer Localizer
<h2>@Localizer["User list"]</h2>
<table class="table">
    @foreach (var user in Model)
    {
        <tr>
            <td>@user.Email</td>
            <td>
                <a class="btn btn-sm btn-primary" asp-action="Edit" asp-route-userid="@user.Id">@Localizer["access right"]</a>
            </td>
        </tr>
    }
</table>